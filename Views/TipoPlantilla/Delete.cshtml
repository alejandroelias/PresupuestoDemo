@model TipoPlantillaModel

@{ ViewData["Title"] = "Borrar Tipo Plantilla";
}


<h3 class="mb-3">¿Esta seguro que desea borrar el tipo de plantilla @Model.TipoPlantilla ?</h3>

<form asp-action="DeleteTipoPlantilla">
    <input type="hidden" asp-for="Id_TipoPlantilla"/>
    <button type="submit" class="btn btn-danger">Borrar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>